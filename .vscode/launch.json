{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Blazor WASM (Hosted) - Chrome (HTTPS)",
            "type": "blazorwasm",
            "request": "launch",
            "browser": "chrome",
            "url": "https://localhost:5001",
            "webRoot": "${workspaceFolder}",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "DOTNET_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:5000;https://localhost:5001"
            }
        },
        {
            "name": ".NET Server (attach) - BlazorDemo.Showcase",
            "type": "coreclr",
            "request": "attach",
            "processName": "BlazorDemo.Showcase",
            "justMyCode": true
        }
    ],
    "compounds": [
        {
            "name": "Blazor WASM + Server (Chrome, HTTPS)",
            "configurations": [
                "Blazor WASM (Hosted) - Chrome (HTTPS)",
                ".NET Server (attach) - BlazorDemo.Showcase"
            ]
        }
    ]
}
